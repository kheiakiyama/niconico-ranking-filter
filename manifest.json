{
  "manifest_version": 2,
  "name": "Niconico Ranking Filter",
  "description": "Display Niconico Ranking Filtered",
  "version": "0.01",
  "permissions": [
    "activeTab",
    "http://www.nicovideo.jp/"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.nicovideo.jp/ranking*"],
      "js": ["myscript.js"],
      "run_at": "document_end"
    }
  ]
}
